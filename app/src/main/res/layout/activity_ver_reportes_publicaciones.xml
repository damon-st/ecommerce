<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.VerReportesPublicaciones">

    <FrameLayout
        android:name="com.damon.ecommerce.FragmentAdministrador.ReportesUsuarios"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/nav_administrador"
        android:id="@+id/reporte_fragement"/>

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/nav_administrador"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/rounded"
        android:elevation="16dp"
        android:padding="8dp"
        app:cnb_menuResource="@menu/menu_administrador" />

</RelativeLayout>